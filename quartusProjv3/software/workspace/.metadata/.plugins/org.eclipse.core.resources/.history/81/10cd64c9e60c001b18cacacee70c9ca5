#include <stdio.h>

#define switches_base 0x084090f0
#define ledg_base 0x084090c0
#define keys_base 0x084090e0

int main(void)
{
	binary test;
	int * green_leds = (int*) ledg_base;
	volatile int * switches = (int*) switches_base;

	while(1)	{
		test ==  *(switches)
		*(green_leds) = *(switches);
	}
  return 0;
}
